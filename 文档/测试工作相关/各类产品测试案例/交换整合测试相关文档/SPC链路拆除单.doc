<script type="text/javascript">
	 	function print1() { 
	 	var data="<?xml version=\"1.0\" encoding=\"utf-8\" ?><Data>";	 	
	 	 	var input = document.getElementsByName('receiving'); 
	 	    var receive=new Array();
	 	    var receive1=new Array();
 		    for(var i=0;i<input.length;i++){
    				var re=input[i].value;
    				receive.push(re);
    				receive1.push(re);
    		}
    		var send=new Array();
    		var send1=new Array();
    		var input2 = document.getElementsByName('sending');
   			for(var i=0;i<input.length;i++){
    			var se=input2[i].value;
    			send.push(se);
    			send1.push(se);
    		}
    		var input = document.getElementsByName('raiseDevice');
    		var raise=new Array();
    		var raise1=new Array();
 		    for(var i=0;i<input.length;i++){
    				var ra=input[i].innerText;	
    				var ra1=input[i].innerText;	
    				var start=ra.indexOf("---->");
    				if(start>0){
    				  ra=ra.substring(0,start);	
    				  ra1=ra1.substring(start+5,ra1.length);	
    				  raise.push(ra);
    				  raise1.push(ra1);
    				}else{
    				  start=ra.indexOf("<----"); 
    				  ra=ra.substring(start+5,ra.length);	
    				  ra1=ra1.substring(0,start);	
    				  raise.push(ra);
    				  raise1.push(ra1);
    				}
    		}
    		var input = document.getElementsByName('sinkDevice');
    		var sink=new Array();
    		var sink1=new Array();
 		    for(var i=0;i<input.length;i++){
    				var si=input[i].innerText;
    				var si1=input[i].innerText;
    				var last=si.indexOf("<----");
    				if(last>0){
    				si=si.substring(last+5,si.length);
    				si1=si1.substring(0,last);
    				sink.push(si);
    				sink1.push(si1);
    				}else{
    				last=si.indexOf("---->");
    				si=si.substring(0,last);
    				si1=si1.substring(last+5,si1.length);
    				sink.push(si);
    				sink1.push(si1);
    				}
    		}
    		var inputNO = document.getElementsByName('circuitNo');	
    		var inputType = document.getElementsByName('jt_bc_code');
    		var inputName = document.getElementsByName('uDeviceName1');
    		var inputNOs=new Array();
    		var texts=new Array();
    		if(inputNO.length>0){
    		var text=inputNO[0].value+inputType[0].value+inputName[0].value;
    		}
    		for(var i=0;i<inputNO.length;i++){
    	 	  inputNOs.push(inputNO[i].value);
    	      texts.push(text);
    	    }		 
    		var printTime=document.getElementById('printTime');
    		var ptime=printTime.innerHTML;
    		var pt=ptime.lastIndexOf("-");
    		ptime=ptime.substring(0,pt+3);
    		var Print="";
    		for(var i=0;i<receive.length;i++){
    		var QRCode="<Print><QRCode>"+receive[i]+"</QRCode><PrintType>1001</PrintType><Text>"+receive[i]+ptime+"</Text><Text>"+raise[i]+"</Text></Print>"+
    		           "<Print><QRCode>"+receive1[i]+"</QRCode><PrintType>1001</PrintType><Text>"+receive1[i]+ptime+"</Text><Text>"+raise1[i]+"</Text></Print>"+
    		           "<Print><QRCode>"+send[i]+"</QRCode><PrintType>1001</PrintType><Text>"+send[i]+ptime+"</Text><Text>"+sink[i]+"</Text></Print>"+
    		           "<Print><QRCode>"+send1[i]+"</QRCode><PrintType>1001</PrintType><Text>"+send1[i]+ptime+"</Text><Text>"+sink1[i]+"</Text></Print>";
    		Print=Print+QRCode;
    		}
    		for(var i=0;i<inputNOs.length;i++){
    		var QRCode="<Print><QRCode>"+inputNOs[i]+"</QRCode><PrintType>1001</PrintType><Text>"+inputNOs[i]+ptime+"</Text><Text>"+texts[i]+"</Text></Print>"   ; 
    		 Print=Print+QRCode;
    		}
    		var  xml=data+Print+"</Data>";
			loadXML(xml);
		}
	</script>
	<script type="text/javascript">
		function loadXML(xml) {
			var openUrl = "LabelPrint.html?r=" + Math.random();
			var targetWindowStyle = "dialogHeight: 550px; dialogWidth: 550px; center: Yes; help: No; resizable: yes; status: yes; scroll:yes";
			if (window.navigator.userAgent.indexOf("Chrome") >= 1) {			
	 			window.open(openUrl,new String(xml),targetWindowStyle);
	  	  	} else {
	 			window.showModalDialog(openUrl, new String(xml), targetWindowStyle);
	  	  	}
		}
</script>
